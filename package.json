{
  "name": "Vessna-Wedding",
  "version": "0.0.0-development",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "build": "rimraf dist && npm run build:amp && npm run build:client && npm run build:server",
    "build:amp": "cross-env NODE_ENV=production webpack --config config/webpack.amp.config.js --progress --hide-modules",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules",
    "watch:amp": "cross-env NODE_ENV=development webpack --config config/webpack.amp.config.js --progress --hide-modules --watch",
    "watch:client": "cross-env NODE_ENV=development webpack --config config/webpack.client.config.js --progress --hide-modules --watch",
    "watch:server": "cross-env NODE_ENV=development webpack --config config/webpack.server.config.js --progress --hide-modules --watch",
    "storybook": "start-storybook --port 9001 --static-dir ./ --config-dir ./src/storybook",
    "storybook:serve": "start-storybook -c ./src/storybook -p 9001 -s ./public",
    "storybook:build": "build-storybook -c ./src/storybook -o .out && mkdir .out/public && cp -R ./public/* .out/public",
    "lint": "eslint \"src/**\" --ext .js --ext .vue --ignore-pattern *.webmanifest --ignore-pattern \"src/styles/*\"",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server",
    "dev": "cross-env NODE_ENV=development node server",
    "dev:macos": "ttab 'mongod --dbpath ~/Documents/Mongo/Vessna-Wedding' && ttab 'npm run watch:server' && ttab 'npm run watch:client' && npm run dev",
    "dev:windows": "start \"\" powershell -NoExit -Command \"mongod --dbpath $(Resolve-Path ~/Documents/Mongo/Vessna-Wedding)\" && start powershell -NoExit -Command \"npm run watch:server\" && start powershell -NoExit -Command \"npm run watch:client\" && start powershell -NoExit -Command \"npm run dev\";",
    "debug": "cross-env NODE_ENV=development nodemon --inspect=0.0.0.0:9229 server",
    "docs": "jsdoc2md server/routes/query/*.js > server/README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/followyouhome/vessna-wedding.git"
  },
  "release": {
    "repositoryUrl": "https://github.com/followyouhome/vessna-wedding.git",
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/git"
    ]
  },
