{
  "name": "Vessna-Wedding",
  "version": "0.0.1",
  "scripts": {
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules",
    "watch:client": "cross-env NODE_ENV=develop webpack --config config/webpack.client.config.js --progress --hide-modules --watch",
    "watch:server": "cross-env NODE_ENV=develop webpack --config config/webpack.server.config.js --progress --hide-modules --watch",
    "lint": "eslint src/** --fix --ext .js --ext .vue",
    "start": "cross-env NODE_ENV=production node server",
    "dev": "cross-env NODE_ENV=develop node server",
    "dev:macos": "ttab 'mongod --dbpath ~/Documents/Mongo/Vessna-Wedding' && ttab 'npm run watch:server' && ttab 'npm run watch:client' && npm run dev",
    "dev:windows": "echo 'todo'",
    "debug": "cross-env NODE_ENV=develop nodemon --inspect=0.0.0.0:9229 server",
    "docs": "jsdoc2md server/routes/query/*.js > server/README.md"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babili-webpack-plugin": "^0.1.2",
    "body-parser": "^1.17.2",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression": "^1.7.0",
    "connect-mongo": "^2.0.0",
    "cookie": "^0.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "flickity": "^2.0.10",
    "forever-monitor": "^1.7.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "in-view": "^0.6.1",
    "ismobilejs": "^0.4.1",
    "isotope-layout": "^3.0.4",
    "json-server": "^0.12.0",
    "keystone": "^4.0.0",
    "keystone-express-sitemap": "^1.2.0",
    "keystone-storage-adapter-azure": "^1.1.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "memory-fs": "^0.4.1",
    "mobile-detect": "^1.4.1",
    "mongodb": "^3.0.0-rc0",
    "node-sass": "^4.9.4",
    "nodemailer": "^4.4.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "sitemap-generator": "^8.0.0",
    "ttab": "^0.6.1",
    "universal-cookie": "^2.0.8",
    "vue": "^2.4.2",
    "vue-cookie": "^1.1.4",
    "vue-lazyload": "^1.1.4",
    "vue-loader": "^13.0.4",
    "vue-meta": "^1.4.2",
    "vue-no-ssr": "^0.2.2",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.4.2",
    "vue-style-loader": "^3.0.1",
    "vue-tabs-component": "^1.4.0",
    "vue-template-compiler": "^2.4.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-sources": "1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.15.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-vue": "^4.2.0",
    "jsdoc-to-markdown": "^4.0.1",
    "nodemon": "^1.15.1"
  }
}
